{
  "name": "paigevogenthaler",
  "version": "0.1.0",
  "description": "personal website",
  "private": true,
  "author": "Paige Vogenthaler",
  "license": "ISC",
  "homepage": "https://pvogenthaler.github.io/",
  "babel": {
    "presets": [
      "env",
      "react",
      "stage-2"
    ]
  },
  "dependencies": {
    "express": "^4.16.3",
    "react": "^16.3.2",
    "react-dom": "^16.3.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "webpack": "4.19.1",
    "webpack-cli": "2.1.4",
    "webpack-dev-server": "^3.1.4"
  },
  "scripts": {
    "build:dev": "cross-env NODE_ENV=development webpack --config ./src/build/webpack.config.js --mode development",
    "build:prod": "cross-env NODE_ENV=production webpack --config ./src/build/webpack.config.js --mode production",
    "deploy:dev": "cross-env NODE_ENV=development webpack-dev-server --config ./src/build/webpack.config.js --mode development",
    "deploy:prod": "npm run build:prod && git subtree push --prefix public origin master",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ./src/build/webpack.config.js --mode development --watch --open"
  }
}
