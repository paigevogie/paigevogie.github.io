    "webpack-cli": "2.1.4",
    "webpack-dev-server": "^3.7.2"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --config ./build/webpack.config.js --mode production",
    "deploy": "npm run build && git subtree push --prefix public origin master",
    "force-deploy": "npm run build && git push origin `git subtree split --prefix public origin $(git rev-parse --symbolic-full-name --abbrev-ref HEAD)`:master --force",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ./build/webpack.config.js --mode development --watch --open"
  }
